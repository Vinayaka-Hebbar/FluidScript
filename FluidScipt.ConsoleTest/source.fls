class Test {
	var _x = 12, _y = 5 + 1;
	var _array = <int>[10,20];
    static val _name = `my first class`;

	get current() : Test {
		return this;
	}

	get x():int {
		return _x;
	}

	get array(): int[] {
		return _array;
	}

	static get name():string {
		return _name;
	}

	set x(value:int) {
		current._x = value;
	}

	func add():int {
		var x = current.array;
		x[0] = 30;
		x[1] = 20;
		return x[0]+x[1];
	}

    impl func toString() : string {
		return _name;
	}
}